extend index 

block website_title 
	title Delta Information Technology | contact

block home_block 
	section#saasio-breadcurmb(class="saasio-breadcurmb-section")
		div(class="container")
			div(class="breadcurmb-title text-center")
				h2 contact
			div(class="breadcurmb-item-list text-center ul-li")
				ul(class="saasio-page-breadcurmb")
					li
						a(href="#") Home
					li
						a(href="#") contact
	//Start of about section
	section(class="str-contact-section relative-position")
		div(class="container")
			.str-section-title.text-center.str-title-center.str-headline
				h2
					| contact us
			div(class="row")
				div(class="col-md-6 col-sm-6 order-sm-2")
					div(class="contact-info px-2")
						div(class="heading-title")
							h3(class="") contact info
							p
								i(class="fas fa-map-marker-alt")
								| Riyadh ( Kingdom of Saudi Arabia). 26 Al Zafer Street, Madkour Station, next to Banque Misr - Haram - Giza
							p
								i(class="fas fa-phone")
								a(href="tel:01000291432") 01000291432 
							p
								i(class="fas fa-envelope")
								a(href="mailto:info@deltaitech.com") info@deltaitech.com
				div(class="col-md-6 col-sm-6")
					div(class="getin_form")
						form(action="")
							div(class="row")
								#result(class="col-md-12 col-sm-12")
								div(class="col-md-12 col-sm-12")
									div(class="form-group")
										label(for="", class="d-none")
										input(class="form-control", type="text", placeholder="Name", name="" required)
								div(class="col-md-12 col-sm-12")
									div(class="form-group")
										label(for="", class="d-none")
										input(class="form-control", type="email", placeholder="Email", name="" required)
								div(class="col-md-6 col-sm-12")
									div(class="form-group")
										label(for="", class="d-none")
										input(class="form-control", type="number", placeholder="Phone", name="" required)
								div(class="col-md-6 col-sm-12")
									div(class="form-group")
										label(for="", class="d-none")
										input(class="form-control", type="text", placeholder="Subject", name="")
								div(class="col-md-12 col-sm-12")
									div(class="form-group")
										label(for="", class="d-none")
										textarea(class="form-control", placeholder="Message", rows="5" name="userMessage")
								div(class="col-md-12 col-sm-12")
									button(type="submit", class="contact_btn gradient-btn w-100") Send
	.google-map-area.position-relative
		.earth3dmap-com
