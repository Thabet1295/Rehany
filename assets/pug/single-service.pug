extend index 

block website_title 
	title Delta Information Technology | single service

block home_block 
	section#saasio-breadcurmb(class="saasio-breadcurmb-section")
		div(class="container")
			div(class="breadcurmb-title text-center")
				h2 services
			div(class="breadcurmb-item-list text-center ul-li")
				ul(class="saasio-page-breadcurmb")
					li
						a(href="#") Home
					li
						a(href="#") services
					li
						a(href="#") Digital marketing
	//Start of feature section
	section.str-feature-section.str-feature-section-page.str-single-service
