extend index 

block website_title 
	title delta | contact

block home_block 
	// Slider Start
	div(class="map-load")
		iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d13815.832610054587!2d31.2238341697754!3d30.038058399999986!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x1458415d057506c3%3A0xb99dc501501775d!2sTest!5e0!3m2!1sen!2seg!4v1625489304310!5m2!1sen!2seg" width="100%" height="400" style="border:0;" allowfullscreen="" loading="lazy")
	// Slider End

	// Contact Us Start
	section#contact-sec(class="contact-sec")
		div(class="container")
			div(class="row")
				div(class="col-12 col-lg-6 contact-description wow slideInLeft", data-wow-delay=".8s")
					div(class="contact-detail wow fadeInLeft")
						div(class="ex-detail")
							span(class="fly-text") CONTACT US
							h4(class="large-heading")
								span(class="heading-1") Get
								span(class="heading-2") In Touch
						p(class="small-text text-center text-md-left")
							| Lorem ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industryâ€™s standard.
						div(class="row location-details text-center text-md-left")
							div(class="col-12 col-md-12 country-1")
								h4(class="heading-text text-left") United States
								ul
									li
										i(class="fas fa-mobile-alt")
										a(href="#") +(34) 609 33 17 54
									li
										i(class="fas fa-envelope")
										a(href="#") email@website.com
									li
										i(class="fas fa-map-marker")
										a(href="#") 201 Oak Street 27 Manchester, USA
				div(class="col-12 col-lg-6 contact-box text-center text-md-left wow slideInRight", data-wow-delay=".8s")
					div(class="c-box wow fadeInRight")
						h4(class="small-heading") Leave Message
						// <p class="small-text">Lorem ipsum is simply dummy text of the printing and typesetting industry. </p>
						form(class="contact-form")
							div(class="row my-form")
								div(class="col-md-12 col-sm-12")
									#result
								div(class="col-12 col-md-6")
									input(type="text", class="form-control", id="candidate_fname", name="firstName", placeholder="First Name", required="required")
								div(class="col-12 col-md-6")
									input(type="text", class="form-control", id="candidate_lname", name="lastName", placeholder="Last Name", required="required")
								div(class="col-12 col-md-6")
									input(type="email", class="form-control", id="user_email", name="userEmail", placeholder="Email", required="required")
								div(class="col-12 col-md-6")
									input(type="text", class="form-control", id="user_subject", name="userSubject", placeholder="Subject", required="required")
								div(class="col-12")
									textarea(class="form-control", id="user_message", name="userMessage", placeholder="Message", rows="7", required="required")
								div(class="col-12")
									button(class="btn green-color-yellow-gradient-btn user-contact contact_btn", type="button")
										| SUBMIT
	// Contact Us End